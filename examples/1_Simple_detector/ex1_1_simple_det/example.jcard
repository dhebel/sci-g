{
	# sets the number of threads
	"nthreads": 4,

	# verbosity
	"verbosity": 2,
	"gsystemv": 0,
	"g4systemv": 0,

	# the ex1_1 system
	"+gsystem": [
		{
			"system":   "./example1_1",
			"factory": "text",
			"variation": "default"
		}
	],

	"+goutput": [
		{
			"format": "ROOT",
			"name": "events.root",
			"type": "event"
		},
		{
			"format": "TEXT",
			"name": "events.txt",
			"type": "event"
		}
	],

	"n": 100

}
